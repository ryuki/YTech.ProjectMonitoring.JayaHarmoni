@model object
@(Html.Kendo().DropDownListFor(m => m)
        .OptionLabel("-Pilih Pekerjaan-")
        .DataValueField("Id")
        .DataTextField("JobName")
        //.CascadeFrom("ddlProject")
        //.BindTo((System.Collections.IEnumerable)ViewData["customers"])
         .HtmlAttributes(new { @style = "width:200px;" })
         .AutoBind(false)
         .DataSource(source =>
         {
             source.Read(read =>
             {
                 read.Action("PopulateWorks", "TProject").Data("ParentProjectInfo");
             })
             .ServerFiltering(true);
         })
)
